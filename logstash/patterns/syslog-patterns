SYSLOGMESSAGE %{GREEDYDATA}
K8STIME I%{MONTHNUM}%{MONTHDAY} %{TIME}
K8SAPP [^\[]+?
K8SAPPINFO \d+ I.+\d+
NGINXERR \*\d+%{DATA},
SYSLOGNGINXERROR nginx_error: %{BBTS:nginx_timestamp} \[%{WORD:nginx_severity}\] %{SYSLOGPID:syslog_pid}#%{SYSLOGPID}:%{SYSLOGMESSAGE:nginx_message}
SYSLOGNGINXACCESS nginx_access: %{SYSLOGMESSAGE:nginx_message}
SYSLOGJENKINS jenkins:? - - - ( @cee: )?%{SYSLOGMESSAGE:jenkins_msg1}
SYSLOGMSG1 %{NOTSPACE:syslog_program}(\[%{SYSLOGPID:syslog_pid}\]): %{SYSLOGMESSAGE:syslog1_message}
SYSLOGMSG2 %{NOTSPACE:syslog_program}: %{SYSLOGMESSAGE:syslog_message}
SYSLOGK8S %{K8SAPP:syslog_program}\[%{K8SAPPINFO:syslog_appinfo}\] %{SYSLOGMESSAGE:micrk8s_message}
DATESTAMP_JENKINS %{MONTH} %{MONTHDAY}, %{YEAR} %{TIME} (AM|PM)?
NGINXERRMSG ([^,]+)
#BBREQUEST %{WORD:http_verb} %{NOTSPACE:http_request} HTTP/%{NUMBER:http_version}
BBREQUEST (?:%{WORD:verb} %{NOTSPACE:request}(?: HTTP/%{NUMBER:httpversion})?)
